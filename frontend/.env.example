# Supabase project credentials (required for auth)
# Find these in: Supabase dashboard → Settings → API
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# Backend API base URL (optional — defaults to http://localhost:8000)
VITE_API_BASE_URL=http://localhost:8000

# Optional (future): switch voice providers without UI change
# browser | backend
VITE_STT_PROVIDER=browser
VITE_TTS_PROVIDER=browser

# Optional (future): map SDK key
VITE_NAVER_MAP_CLIENT_ID=your_naver_map_client_id
